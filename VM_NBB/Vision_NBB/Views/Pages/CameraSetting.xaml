                                        Style="{StaticResource txtboxMesage2}" Text="{Binding vmGlobalDataModel.FixtureMinPointX}" ></TextBox>

                                <Label Content=" X偏移最大值" Style="{StaticResource MessageLabel}"  MinWidth="80"></Label>
